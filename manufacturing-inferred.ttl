    skos:example "H₂0, C₆H₁₂O₆, CH₄"@en ;
    skos:prefLabel "Molecule"@en ;
    ns1:EMMO_8a137e9f_579c_4e28_baca_e8980eb0c3db "DisjointUnionOf: Heteronuclear, Homonuclear"^^xsd:string ;
